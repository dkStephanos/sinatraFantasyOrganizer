<h1>Welcome, <%=@user.username%></h1>

<h3>Teams:</h3>

<ul>
  <%@teams.each do |team|%>
    <li><a href="/teams/<%=team.slug%>"><%=team.name%></a></li>
  <%end%>
</ul>

<br></br>

<form class="" action="/teams/new" method="get">
  <button type="submit" name="New Team">Create New Team</button>
</form>

<br></br>

<form class="" action="/logout" method="get">
  <button type="submit" name="Logout">Logout</button>
</form>
